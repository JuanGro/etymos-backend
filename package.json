{
  "name": "etymos-backend",
  "version": "1.0.1",
  "description": "Backend for Etymos App",
  "main": "index.ts",
  "scripts": {
    "start": "nodemon -w src --ext ts --exec ts-node src/index.ts",
    "start:development": "npm run migrate && npm start",
    "start:production": "ts-node ./src/index.ts",
    "build": "tsc -p tsconfig.json",
    "build:clean": "rimraf ./build && npm run build",
    "test": "npm run seed:run:test && DATABASE_NAME=etymos-testing jest --config jest.config.ts --detectOpenHandles",
    "lint": "tslint -p tsconfig.json",
    "prettier": "prettier --write \"**\"",
    "migrate": "ts-node ./node_modules/typeorm/cli.js migration:run",
    "makemigrations": "ts-node ./node_modules/typeorm/cli.js migration:generate -n",
    "createmigration": "ts-node ./node_modules/typeorm/cli.js migration:create -n",
    "showmigrations": "ts-node ./node_modules/typeorm/cli.js migration:show",
    "revertmigrations": "ts-node ./node_modules/typeorm/cli.js migration:revert",
    "schema:drop": "ts-node ./node_modules/typeorm/cli.js schema:drop",
    "schema:sync": "ts-node ./node_modules/typeorm/cli.js schema:sync",
    "migrate:test": "DATABASE_NAME=etymos-testing ts-node ./node_modules/typeorm/cli.js migration:run",
    "showmigrations:test": "DATABASE_NAME=etymos-testing ts-node ./node_modules/typeorm/cli.js migration:show",
    "revertmigrations:test": "DATABASE_NAME=etymos-testing ts-node ./node_modules/typeorm/cli.js migration:revert",
    "schema:drop:test": "DATABASE_NAME=etymos-testing ts-node ./node_modules/typeorm/cli.js schema:drop",
    "schema:sync:test": "DATABASE_NAME=etymos-testing ts-node ./node_modules/typeorm/cli.js schema:sync",
    "seed:config:test": "DATABASE_NAME=etymos-testing ts-node ./node_modules/typeorm-seeding/dist/cli.js config",
    "seed:run:test": "DATABASE_NAME=etymos-testing ts-node ./node_modules/typeorm-seeding/dist/cli.js seed --seed Testing"
  },
  "keywords": [],
  "author": "Juan Manuel Guerrero Hern√°ndez",
  "license": "ISC",
  "dependencies": {
    "@sentry/node": "^5.29.2",
    "@sentry/tracing": "^5.29.2",
    "apollo-server": "^2.19.1",
    "class-validator": "^0.13.1",
    "graphql": "^15.5.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "ts-node": "^9.1.1",
    "type-graphql": "^1.1.1",
    "typeorm": "^0.2.29"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.12.12",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/faker": "^5.1.5",
    "@types/jest": "^26.0.20",
    "jest": "^26.6.3",
    "jest-config": "^26.6.3",
    "nodemon": "^2.0.6",
    "ts-jest": "^26.4.4",
    "tslint": "^6.1.3",
    "typeorm-seeding": "^1.6.1",
    "typescript": "^4.1.3"
  }
}
